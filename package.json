{
    "name": "advpl-codex",
    "displayName": "AdvPL Codex",
    "description": "AdvPL language snippets and reference",
    "version": "0.0.3",
    "publisher": "vlopes11",
    "icon": "img/advpl-codex-128.png",
    "license": "MIT",
    "readme": "README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/vlopes11/advpl-codex"
    },
    "bugs": {
        "url": "https://github.com/vlopes11/advpl-codex/issues"
    },
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "advpl",
                "path": "./snippets/advpl.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}